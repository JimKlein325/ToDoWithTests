@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<TravelBlog.Models.Experience>

<h2>Travel Blog</h2>
<hr />

<ul>
@foreach (var listExperience in Model)
{
    <li>@Html.ActionLink(listExperience.Title,"Details", new { id = listExperience.ExperienceId }) | @Html.ActionLink("Edit","Edit",new { id = listExperience.ExperienceId}) | @Html.ActionLink("Delete", "Delete", new { id = listExperience.ExperienceId })</li>
}
</ul>
<p>@Html.ActionLink("Create New", "Create") | @Html.ActionLink("Back to Locations","Index", "Locations")</p>